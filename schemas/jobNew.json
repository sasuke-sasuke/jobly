{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "salary",
        "equity",
        "company-handle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Software Developer"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                120000
            ]
        },
        "equity": {
            "type": "number",
            "default": 0.0,
            "title": "The equity Schema",
            "examples": [
                0.7
            ]
        },
        "company-handle": {
            "type": "string",
            "default": "",
            "title": "The company-handle Schema",
            "examples": [
                "martinez-daniels"
            ]
        }
    },
    "examples": [{
        "title": "Software Developer",
        "salary": 120000,
        "equity": 0.7,
        "company-handle": "martinez-daniels"
    }]
}